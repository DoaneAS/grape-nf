// Process configuration
process {

    $bigwig {
        tool = 'CRG'
        modules = ['SAMtools', 'BEDTools', 'KentUtils']
        profile = { '-' + readStrand }
        views = {
            bwStrandViews[readStrand]
        }
        cpus = 4
        memory = '15G'
        time = '12h'
    }

}
